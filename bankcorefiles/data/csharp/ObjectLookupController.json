[
"{SetValue(Item:{ObjectName})}",
"{SetValue(Items:{Plural(ObjectName)})}",
"{SetValue(LItem:{Lower({ObjectName})})}",
"{SetValue(LItems:{Plural({LItem})})}",
"using System;",
"using System.Collections.Generic;",
"using System.Data;",
"using System.Data.Entity;",
"using System.Data.Entity.Infrastructure;",
"using System.Linq;",
"using System.Net;",
"using System.Net.Http;",
"using System.Web.Http;",
"using System.Web.Http.Description;",
"",
"using BankViewModel;",
"",
"namespace BankWEB",
	"//*-------------------------------------------------------------------------*",
	"//* {Item}LookupsController {SpaceTo(75)}*",
	"//*-------------------------------------------------------------------------*",
	"/// <summary>",
	"/// Web API 2 Controller for transient {LItem} lookup objects.",
	"/// </summary>",
	"public class {Item}LookupsController : ApiController",
	"{",
	"{IncIndent}",
		"{Include(PrivateSection.json)}",
		"private {Items}Controller mEntity = new {Items}Controller();",
		"",
		"{Include(ProtectedSection.json)}",
		"{Include(PublicSection.json)}",
		"//*-----------------------------------------------------------------------*",
		"//* Get{Item}Lookup {SpaceTo(73)}*",
		"//*-----------------------------------------------------------------------*",
		"/// <summary>",
		"/// GET: api/{Item}Lookups/5",
		"/// </summary>",
		"/// <remarks>",
		"/// <para>",
		"/// Return the ID and default text of the specified {LItem}.",
		"/// </para>",
		"/// <para>",
		"/// Even with a single entry, it is important to return an array for",
		"/// Ajax client.",
		"/// </para>",
		"/// </remarks>",
		"[[ResponseType(typeof(IDTextItem[[]]))]]",
		"public IHttpActionResult Get{Item}Lookup(int id)",
		"{",
		"{IncIndent}",
			"IDTextItem di = mEntity.Lookup(id);",
			"IHttpActionResult rv = null;",
			"",
			"if(di != null)",
			"{",
			"{IncIndent}",
				"rv = Ok(di);",
				"{DecIndent}",
			"}",
			"else",
			"{",
			"{IncIndent}",
				"rv = NotFound();",
				"{DecIndent}",
			"}",
			"return rv;",
			"{DecIndent}",
		"}",
		"//*-----------------------------------------------------------------------*",
		"",
		"//*-----------------------------------------------------------------------*",
		"//* Get{Item}Lookups {SpaceTo(73)}*",
		"//*-----------------------------------------------------------------------*",
		"/// <summary>",
		"/// GET: api/{Item}Lookups",
		"/// </summary>",
		"/// <remarks>",
		"/// Return all {LItem} lookups.",
		"/// </remarks>",
		"public IQueryable<IDTextItem> Get{Item}Lookups()",
		"{",
		"{IncIndent}",
			"return mEntity.Lookups().AsQueryable<IDTextItem>();",
			"{DecIndent}",
		"}",
		"//*-----------------------------------------------------------------------*",
		"{DecIndent}",
	"}",
	"{DecIndent}",
"}",
""
