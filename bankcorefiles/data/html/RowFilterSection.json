[
	"<!-- {ObjectName} Filtering -->",
	"<div class=\"tabAccordian\">",
	"{IncIndent}",
		"<h3>Row Filter Settings</h3>",
		"<div id=\"filt{Plural({ObjectName})}\" class=\"rowFilter\">",
		"{IncIndent}",
			"<div class=\"head\">Row Filtering</div>",
			"<p class=\"small\">Enter all or part of a value to {Continue}",
			"look for in each desired column.</p>",
			"{Include(RowFilterItems.json)}",
			"<input type=\"button\" {Continue}",
			"id=\"btnfilt{Lower([Object:Abbrev3])}Apply\" value=\"Apply Filter\">",
			"<input type=\"button\" {Continue}",
			"id=\"btnfilt{Lower([Object:Abbrev3])}Clear\" value=\"Clear Values\">",
			"{DecIndent}",
		"</div>",
		"{DecIndent}",
	"</div>",
	"<!-- /{ObjectName} Filtering -->"
]
